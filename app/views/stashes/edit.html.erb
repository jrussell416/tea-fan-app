Edit an entry into your tea collection
 
<%= form_for @stash do |f| %>
    <div class="mb-3">
    <%= f.label :tea_name %>
    <%= f.text_field :tea_name %>
    </div>
    <div class="mb-3">
    <%= f.label :tea_type %>
    <%= f.text_field :tea_type %>
    </div>
    <div class="mb-3">
    <%= f.label :purchased_at %>
    <%= f.text_field :purchased_at %>
    </div>
    <div class="mb-3">
    <%= f.label :stash_amount %>
    <%= f.text_field :stash_amount %>
    </div>
    <div>
    <%= f.label :notes %>
    <%= f.text_area :notes, size: "50x10" %>
    </div>
    <%= f.submit %>
<% end %>