<%= render partial: "shared/navbar" %>

<h2>All Users</h2>

<table class="table">
  <thead>
  <tr>
    <th scope="col">User Name</th>
    <th scope="col">Email</th>
    <th scope="col">Admin</th>
  </tr>
  </thead>
  <tbody>

  <tr>
    <% @users.each do |user| %>

      <td><%= user.user_name %></td>
      <td><%= user.email %></td>
      <td><%= user.admin %></td>
      </tr>

  <% end %>
  </tbody>
</table>


<%= link_to "Dashboard Main", dashboards_path %>






